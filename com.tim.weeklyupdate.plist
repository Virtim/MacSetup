<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.tim.weeklyupdate</string>

  <key>ProgramArguments</key>
  <array>
    <string>cd ~/Documents/MacSetup && make monthlyupdate</string>
  </array>

  <key>Nice</key>
  <integer>1</integer>
  <key>StartCalendarInterval</key>
    <dict>
        <key>Day</key>
        <integer>1</integer>
    </dict>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardErrorPath</key>
  <string>~/Desktop/tmp/AlTest1.err</string>

  <key>StandardOutPath</key>
  <string>~/Desktop/tmp/AlTest1.out</string>
</dict>
</plist>
