<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.tim.weeklyupdate</string>

  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/make</string>
    <string>weeklyupdate</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/tim/dev/MacSetup</string>

  <key>Nice</key>
  <integer>1</integer>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Weekday</key>
    <integer>1</integer>
  </dict>

  <key>RunAtLoad</key>
  <false/>

  <key>StandardErrorPath</key>
  <string>/Users/tim/Library/Logs/weeklyupdate.err</string>

  <key>StandardOutPath</key>
  <string>/Users/tim/Library/Logs/weeklyupdate.log</string>
</dict>
</plist>
